{
  "name": "time-ticker",
  "version": "0.0.17",
  "description": "Web Component that fires event periodically",
  "main": "time-ticker.js",
  "module": "time-ticker.js",
  "dependencies": {
    "xtal-element": "0.0.471",
    "trans-render": "0.0.472"
  },
  "devDependencies": {
    "xtal-test": "0.0.55",
    "on-to-me": "0.0.49",
    "custom-elements-manifest": "1.0.0",
    "typescript-json-schema": "0.53.0",
    "may-it-be": "0.0.128",
    "npm-watch": "0.11.0"
  },
  "watch": {
    "build-schema": "types.d.ts",
    "build-manifest": "schema.json"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node node_modules/xtal-test/serve.js",
